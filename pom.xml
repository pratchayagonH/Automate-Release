<?xml version='1.0' encoding='utf-8'?>
<ns0:project xmlns:ns0="http://maven.apache.org/POM/4.0.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://maven.apache.org/POM/4.0.0          http://maven.apache.org/xsd/maven-4.0.0.xsd">
    <ns0:modelVersion>4.0.0</ns0:modelVersion>

    
    <ns0:parent>
        <ns0:groupId>org.springframework.boot</ns0:groupId>
        <ns0:artifactId>spring-boot-starter-parent</ns0:artifactId>
        <ns0:version>3.2.0</ns0:version>
        <ns0:relativePath />
    </ns0:parent>

    
    <ns0:groupId>com.automaterelease</ns0:groupId>
    <ns0:artifactId>automate-release</ns0:artifactId>
    <ns0:version>0.2.0</ns0:version>
    <ns0:packaging>jar</ns0:packaging>

    <ns0:name>Automate-Release</ns0:name>
    <ns0:description>Spring Boot Application for Automated Release Management</ns0:description>

    
    <ns0:properties>
        
        <ns0:java.version>17</ns0:java.version>
        <ns0:maven.compiler.source>17</ns0:maven.compiler.source>
        <ns0:maven.compiler.target>17</ns0:maven.compiler.target>

        
        <ns0:project.build.sourceEncoding>UTF-8</ns0:project.build.sourceEncoding>
        <ns0:project.reporting.outputEncoding>UTF-8</ns0:project.reporting.outputEncoding>

        
        <ns0:lombok.version>1.18.30</ns0:lombok.version>
        <ns0:springdoc.version>2.2.0</ns0:springdoc.version>
        <ns0:jwt.version>0.11.5</ns0:jwt.version>
    </ns0:properties>

    
    <ns0:dependencies>
        
        <ns0:dependency>
            <ns0:groupId>org.springframework.boot</ns0:groupId>
            <ns0:artifactId>spring-boot-starter-web</ns0:artifactId>
        </ns0:dependency>

        
        <ns0:dependency>
            <ns0:groupId>org.springframework.boot</ns0:groupId>
            <ns0:artifactId>spring-boot-starter-data-jpa</ns0:artifactId>
        </ns0:dependency>

        
        <ns0:dependency>
            <ns0:groupId>org.springframework.boot</ns0:groupId>
            <ns0:artifactId>spring-boot-starter-validation</ns0:artifactId>
        </ns0:dependency>

        
        <ns0:dependency>
            <ns0:groupId>org.springframework.boot</ns0:groupId>
            <ns0:artifactId>spring-boot-starter-security</ns0:artifactId>
        </ns0:dependency>

        
        <ns0:dependency>
            <ns0:groupId>org.springframework.boot</ns0:groupId>
            <ns0:artifactId>spring-boot-starter-actuator</ns0:artifactId>
        </ns0:dependency>

        
        <ns0:dependency>
            <ns0:groupId>com.h2database</ns0:groupId>
            <ns0:artifactId>h2</ns0:artifactId>
            <ns0:scope>runtime</ns0:scope>
        </ns0:dependency>

        
        

        
        

        
        <ns0:dependency>
            <ns0:groupId>org.projectlombok</ns0:groupId>
            <ns0:artifactId>lombok</ns0:artifactId>
            <ns0:version>${lombok.version}</ns0:version>
            <ns0:optional>true</ns0:optional>
        </ns0:dependency>

        
        <ns0:dependency>
            <ns0:groupId>org.springdoc</ns0:groupId>
            <ns0:artifactId>springdoc-openapi-starter-webmvc-ui</ns0:artifactId>
            <ns0:version>${springdoc.version}</ns0:version>
        </ns0:dependency>

        
        <ns0:dependency>
            <ns0:groupId>io.jsonwebtoken</ns0:groupId>
            <ns0:artifactId>jjwt-api</ns0:artifactId>
            <ns0:version>${jwt.version}</ns0:version>
        </ns0:dependency>
        <ns0:dependency>
            <ns0:groupId>io.jsonwebtoken</ns0:groupId>
            <ns0:artifactId>jjwt-impl</ns0:artifactId>
            <ns0:version>${jwt.version}</ns0:version>
            <ns0:scope>runtime</ns0:scope>
        </ns0:dependency>
        <ns0:dependency>
            <ns0:groupId>io.jsonwebtoken</ns0:groupId>
            <ns0:artifactId>jjwt-jackson</ns0:artifactId>
            <ns0:version>${jwt.version}</ns0:version>
            <ns0:scope>runtime</ns0:scope>
        </ns0:dependency>

        
        <ns0:dependency>
            <ns0:groupId>com.fasterxml.jackson.datatype</ns0:groupId>
            <ns0:artifactId>jackson-datatype-jsr310</ns0:artifactId>
        </ns0:dependency>

        
        <ns0:dependency>
            <ns0:groupId>org.springframework.boot</ns0:groupId>
            <ns0:artifactId>spring-boot-devtools</ns0:artifactId>
            <ns0:scope>runtime</ns0:scope>
            <ns0:optional>true</ns0:optional>
        </ns0:dependency>

        
        <ns0:dependency>
            <ns0:groupId>org.springframework.boot</ns0:groupId>
            <ns0:artifactId>spring-boot-configuration-processor</ns0:artifactId>
            <ns0:optional>true</ns0:optional>
        </ns0:dependency>

        
        <ns0:dependency>
            <ns0:groupId>org.springframework.boot</ns0:groupId>
            <ns0:artifactId>spring-boot-starter-test</ns0:artifactId>
            <ns0:scope>test</ns0:scope>
        </ns0:dependency>

        <ns0:dependency>
            <ns0:groupId>org.springframework.security</ns0:groupId>
            <ns0:artifactId>spring-security-test</ns0:artifactId>
            <ns0:scope>test</ns0:scope>
        </ns0:dependency>
    </ns0:dependencies>

    
    <ns0:build>
        <ns0:finalName>${project.artifactId}</ns0:finalName>

        <ns0:plugins>
            
            <ns0:plugin>
                <ns0:groupId>org.springframework.boot</ns0:groupId>
                <ns0:artifactId>spring-boot-maven-plugin</ns0:artifactId>
                <ns0:configuration>
                    <ns0:excludes>
                        <ns0:exclude>
                            <ns0:groupId>org.projectlombok</ns0:groupId>
                            <ns0:artifactId>lombok</ns0:artifactId>
                        </ns0:exclude>
                    </ns0:excludes>
                </ns0:configuration>
            </ns0:plugin>

            
            <ns0:plugin>
                <ns0:groupId>org.apache.maven.plugins</ns0:groupId>
                <ns0:artifactId>maven-compiler-plugin</ns0:artifactId>
                <ns0:configuration>
                    <ns0:source>${java.version}</ns0:source>
                    <ns0:target>${java.version}</ns0:target>
                    <ns0:annotationProcessorPaths>
                        <ns0:path>
                            <ns0:groupId>org.projectlombok</ns0:groupId>
                            <ns0:artifactId>lombok</ns0:artifactId>
                            <ns0:version>${lombok.version}</ns0:version>
                        </ns0:path>
                    </ns0:annotationProcessorPaths>
                </ns0:configuration>
            </ns0:plugin>

            
            <ns0:plugin>
                <ns0:groupId>org.apache.maven.plugins</ns0:groupId>
                <ns0:artifactId>maven-surefire-plugin</ns0:artifactId>
                <ns0:configuration>
                    <ns0:includes>
                        <ns0:include>**/*Test.java</ns0:include>
                        <ns0:include>**/*Tests.java</ns0:include>
                    </ns0:includes>
                </ns0:configuration>
            </ns0:plugin>

            
            <ns0:plugin>
                <ns0:groupId>org.apache.maven.plugins</ns0:groupId>
                <ns0:artifactId>maven-resources-plugin</ns0:artifactId>
                <ns0:configuration>
                    <ns0:encoding>UTF-8</ns0:encoding>
                </ns0:configuration>
            </ns0:plugin>
        </ns0:plugins>
    </ns0:build>

    
    <ns0:profiles>
        
        <ns0:profile>
            <ns0:id>dev</ns0:id>
            <ns0:activation>
                <ns0:activeByDefault>true</ns0:activeByDefault>
            </ns0:activation>
            <ns0:properties>
                <ns0:spring.profiles.active>dev</ns0:spring.profiles.active>
            </ns0:properties>
        </ns0:profile>

        
        <ns0:profile>
            <ns0:id>prod</ns0:id>
            <ns0:properties>
                <ns0:spring.profiles.active>prod</ns0:spring.profiles.active>
            </ns0:properties>
            <ns0:build>
                <ns0:plugins>
                    
                    <ns0:plugin>
                        <ns0:groupId>org.apache.maven.plugins</ns0:groupId>
                        <ns0:artifactId>maven-surefire-plugin</ns0:artifactId>
                        <ns0:configuration>
                            <ns0:skipTests>false</ns0:skipTests>
                        </ns0:configuration>
                    </ns0:plugin>
                </ns0:plugins>
            </ns0:build>
        </ns0:profile>

        
        <ns0:profile>
            <ns0:id>test</ns0:id>
            <ns0:properties>
                <ns0:spring.profiles.active>test</ns0:spring.profiles.active>
            </ns0:properties>
        </ns0:profile>
    </ns0:profiles>

    
    <ns0:repositories>
        <ns0:repository>
            <ns0:id>spring-milestones</ns0:id>
            <ns0:name>Spring Milestones</ns0:name>
            <ns0:url>https://repo.spring.io/milestone</ns0:url>
            <ns0:snapshots>
                <ns0:enabled>false</ns0:enabled>
            </ns0:snapshots>
        </ns0:repository>
    </ns0:repositories>

    <ns0:pluginRepositories>
        <ns0:pluginRepository>
            <ns0:id>spring-milestones</ns0:id>
            <ns0:name>Spring Milestones</ns0:name>
            <ns0:url>https://repo.spring.io/milestone</ns0:url>
            <ns0:snapshots>
                <ns0:enabled>false</ns0:enabled>
            </ns0:snapshots>
        </ns0:pluginRepository>
    </ns0:pluginRepositories>

</ns0:project>